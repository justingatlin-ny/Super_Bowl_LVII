{
  "name": "super-bowl-lvii",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "pm2 start --only superbowl-dev --no-daemon",
    "server": "pm2 start --only superbowl-prod --env production",
    "delete": "pm2 delete superbowl-dev",
    "start": "concurrently \"yarn webpack:server\" \"yarn webpack:dom\"",
    "webpack:server": "NODE_ENV=development webpack --config webpack.server.js",
    "webpack:dom": "NODE_ENV=development webpack --config webpack.dom.js",
    "prod:server": "NODE_ENV=production webpack --config webpack.server.js",
    "prod:dom": "NODE_ENV=production webpack --config webpack.dom.js",
    "webpack:prod": "concurrently \"yarn prod:server\" \"yarn prod:dom\"",
    "clean": "rm -rf htdocs/*",
    "flush": "pm2 flush",
    "status": "pm2 status"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "amazon-mws": "^0.0.27",
    "axios": "^0.21.1",
    "body-parser": "^1.19.1",
    "cookie-parser": "~1.4.4",
    "crypto-js": "^4.0.0",
    "ejs": "^3.1.5",
    "ejs-loader": "^0.5.0",
    "express": "^4.17.2",
    "helmet": "^4.4.1",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.10.0",
    "mysql2": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.12.18",
    "react-is": "^17.0.1",
    "rethinkdb": "^2.4.2",
    "socket.io": "^3.1.0",
    "socket.io-client": "^3.1.0",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2",
    "xmldom": "^0.4.0",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.6.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-node": "^0.0.0",
    "webpack-node-externals": "^2.5.2"
  }
}
